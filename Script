local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "kean", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

--[[
Name = <string> - The name of the UI.
HidePremium = <bool> - Whether or not the user details shows Premium status or not.
SaveConfig = <bool> - Toggles the config saving in the UI.
ConfigFolder = <string> - The name of the folder where the configs are saved.
IntroEnabled = <bool> - Whether or not to show the intro animation.
IntroText = <string> - Text to show in the intro animation.
IntroIcon = <string> - URL to the image you want to use in the intro animation.
Icon = <string> - URL to the image you want displayed on the window.
CloseCallback = <function> - Function to execute when the window is closed.
]]
local Tab = Window:MakeTab({
	Name = "blox fruit",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

--[[
Name = <string> - The name of the tab.
Icon = <string> - The icon of the tab.
PremiumOnly = <bool> - Makes the tab accessible to Sirus Premium users only.
]]

local Section = Tab:AddSection({
	Name = "Follow me Tik Tok @zylos_store0"
})

--[[
Name = <string> - The name of the section.
]]

OrionLib:MakeNotification({
	Name = "home",
	Content = "Notification content... what will it say??",
	Image = "rbxassetid://4483345998",
	Time = 5
})

--[[
Title = <string> - The title of the notification.
Content = <string> - The content of the notification.
Image = <string> - The icon of the notification.
Time = <number> - The duration of the notfication.
]]

Tab:AddButton({
	Name = "redz Hub",
	Callback = function()
      		loadstring(game:HttpGet("https://raw.githubusercontent.com/realredz/BloxFruits/refs/heads/main/Source.lua"))()
  	end    
})

--[[
Name = <string> - The name of the button.
Callback = <function> - The function of the button.
]]

Tab:AddButton({
	Name = "Banana Hub no key",
	Callback = function()
      		repeat wait() until game:IsLoaded() and game.Players.LocalPlayer getgenv().Key = "TZJTnkbRpwrhlIoxRakphyBglcleIAst" loadstring(game:HttpGet("https://raw.githubusercontent.com/obiiyeuem/vthangsitink/main/BananaHub.lua"))()
  	end    
})

Tab:AddButton({
	Name = "CokkaHub",
	Callback = function()
      		loadstring(game:HttpGet"https://raw.githubusercontent.com/UserDevEthical/Loadstring/main/CokkaHub.lua")()
  	end    
})

local Section = Tab:AddSection({
	Name = "Script made by me"
})

Tab:AddButton({
	Name = "on Activated luck 2x",
	Callback = function()
      		-- إنشاء واجهة GUI جديدة
local ScreenGui = Instance.new("ScreenGui")
local TextLabel = Instance.new("TextLabel")

-- إعداد ScreenGui
ScreenGui.Name = "LuckGui"
ScreenGui.Parent = game.CoreGui -- يضمن ظهورها في واجهة المستخدم

-- إعداد TextLabel
TextLabel.Parent = ScreenGui
TextLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0) -- لون خلفية أسود
TextLabel.BackgroundTransparency = 0.5 -- شفافية الخلفية
TextLabel.Size = UDim2.new(1, 0, 1, 0) -- يجعل النص يغطي الشاشة بالكامل
TextLabel.Position = UDim2.new(0, 0, 0, 0) -- توسيط النص بالكامل
TextLabel.Text = " on Activated luck 2x"
TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255) -- لون النص أبيض
TextLabel.TextScaled = true -- يجعل النص يتكيف مع الحجم
TextLabel.Font = Enum.Font.SourceSansBold -- نوع الخط

-- إظهار النص لمدة ثانيتين ثم إزالته
wait(2)
ScreenGui:Destroy()
  	end    
})

--[[
Name = <string> - The name of the button.
Callback = <function> - The function of the button.
]]

--[[
Name = <string> - The name of the button.
Callback = <function> - The function of the button.
]]

--[[
Name = <string> - The name of the button.
Callback = <function> - The function of the button.
]]

local Tab = Window:MakeTab({
	Name = "blox fruits but very good",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Follow me Tik Tok @zylos_store0"
})


Tab:AddButton({
	Name = "Vanta Hub op",
	Callback = function()
      		loadstring(game:HttpGet("https://rawscripts.net/raw/UPD-Blox-Fruit-But-Very-Good-Vanta-Hub-OP-GIVE-ALL-26764"))()
  	end    
})

